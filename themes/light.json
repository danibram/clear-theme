{
  "$schema": "vscode://schemas/color-theme",
  "tokenColors": [
    {
      "scope": [
        "comment",
        "comment.block",
        "comment.block.documentation",
        "comment.line",
        "meta.toc-list",
        "punctuation.definition.comment",
        "string.quoted.docstring",
        "variable.parameter.function"
      ],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#b2b2b2"
      }
    },
    {
      "scope": [
        "entity.name.class",
        "entity.name.enum",
        "entity.name.interface",
        "entity.name.section",
        "entity.name.struct",
        "entity.name.tag",
        "entity.name.trait",
        "entity.name.type",
        "entity.name.union",
        "entity.other",
        "meta.function-call.generic.python"
      ],
      "settings": {
        "fontStyle": "bold",
        "foreground": "#000000"
      }
    },
    {
      "scope": [
        "keyword.control",
        "keyword.control.conditional",
        "keyword.control.import",
        "keyword.function.go",
        "keyword.import.go",
        "keyword.package.go",
        "keyword.var.go"
      ],
      "settings": {
        "fontStyle": "bold italic"
      }
    },
    {
      "scope": [
        "entity.name.tag.localname.xml",
        "keyword.control",
        "keyword.operator",
        "meta.tag.xml"
      ],
      "settings": {
        "fontStyle": "bold",
        "foreground": "#000"
      }
    },
    {
      "scope": [
        "string.quoted.double",
        "string.quoted.other",
        "string.quoted.single",
        "string.quoted.triple",
        "string.regexp",
        "string.unquoted",
        "string.template.ts",
        "string.template.js",
        "string.double"
      ],
      "settings": {
        "fontStyle": "",
        "foreground": "#22863A"
      }
    },
    {
      "scope": ["markup.bold.markdown"],
      "settings": {
        "fontStyle": "bold"
      }
    },
    {
      "scope": ["markup.inline.raw"],
      "settings": {
        "foreground": "#333333"
      }
    },
    {
      "scope": ["markup.italic"],
      "settings": {
        "fontStyle": "italic"
      }
    },
    {
      "scope": [
        "markup.underline",
        "punctuation.definition.string.begin",
        "punctuation.definition.string.end",
        "punctuation.separator",
        "punctuation.terminator",
        "punctuation.definition",
        "meta.brace"
      ],
      "settings": {
        "fontStyle": "bold",
        "foreground": "#333"
      }
    },
    {
      "scope": ["meta.link", "meta.paragraph", "meta.property-value"],
      "settings": {
        "foreground": "#333"
      }
    },
    {
      "scope": ["storage.modifier", "storage.type"],
      "settings": {
        "fontStyle": "bold italic",
        "foreground": "#000000"
      }
    },
    {
      "scope": [
        "support.type.property-name.css",
        "support.type.vendored.property-name.css",
        "variable.other.readwrite.ts",
        "variable.other.readwrite.Js"
      ],
      "settings": {
        "foreground": "#000000"
      }
    },

    {
      "name": "JSON keys",
      "scope": [
        "source.json meta.structure.dictionary.json support.type.property-name.json"
      ],
      "settings": {
        "fontStyle": "bold"
      }
    },
    {
      "name": "const",
      "scope": ["variable.other.constant", "variable.other.readwrite"],
      "settings": {
        "foreground": "#af3a03"
      }
    },
    {
      "name": "ReasonML property",
      "scope": [
        "source.reason support.property-value",
        "source.reason entity.name.filename"
      ],
      "settings": {
        "foreground": "#af3a03"
      }
    },
    {
      "name": "ReasonML property",
      "scope": [
        "source.reason constant.language.unit",
        "source.reason entity.name.function"
      ],
      "settings": {
        "fontStyle": "bold",
        "foreground": "#000"
      }
    },
    {
      "name": "types not green",
      "scope": ["source.reason string.regexp"],
      "settings": {
        "foreground": "#000"
      }
    },
    {
      "name": "only this green",
      "scope": ["source.reason string.double string.regexp"],
      "settings": {
        "foreground": "#22863A"
      }
    },

    //Shell
    {
      "scope": "keyword.operator.logical",
      "settings": {
        "foreground": "#af3a03"
      }
    },
    {
      "scope": "punctuation.definition.logical-expression",
      "settings": {
        "foreground": "#af3a03"
      }
    },
    {
      "scope": "string.inperpolated.dollar.shell",
      "settings": {
        "foreground": "#af3a03"
      }
    },
    {
      "scope": [
        "string.interpolated.dollar.shell",
        "string.interpolated.backtick.shell"
      ],
      "settings": {
        "foreground": "#427b58"
      }
    },
    // HTML / XML ----------------------------------------
    {
      "scope": ["punctuation.definition.tag"],
      "settings": {
        "foreground": "#076678"
      }
    },
    {
      "scope": ["text.html entity.name.tag", "text.html punctuation.tag"],
      "settings": {
        "foreground": "#427b58",
        "fontStyle": "bold"
      }
    },

    // HJSON ----------------------------------------
    {
      "scope": [
        "string.quoted.none.hjson",
        "string.quoted.multiline.hjson",
        "constant.language.hjson"
      ],
      "settings": {
        "fontStyle": "",
        "foreground": "#22863A"
      }
    },
    {
      "scope": ["support.type.property-name.hjson"],
      "settings": {
        "fontStyle": "bold",
        "foreground": "#000"
      }
    },

    // JS OVERRRIDE

    {
      "scope": [
        "source.ts variable.other.constant",
        "source.js variable.other.constant"
      ],
      "settings": {
        "foreground": "#000"
      }
    }
  ],
