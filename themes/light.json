{
  "$schema": "vscode://schemas/color-theme",
  "include": "./_light_colors.json",
  "semanticHighlighting": true,
  "semanticTokenColors": {
    // "newOperator": "#22863A",
    // "stringLiteral": "#076678",
    // "customLiteral": "#22863A",
    // "numberLiteral": "#427b58"
    "namespace": "#000",
    "type": "#000000",
    "type.defaultLibrary": "#000000",
    "struct": "#000000",
    "class": "#000000",
    "class.defaultLibrary": "#000000",
    "interface": "#000000",
    "enum": "#000000",
    "function": "#000000",
    "function.defaultLibrary": "#000000",
    "method": "#000000",
    "macro": "#000000",
    "variable": "#000000",
    "variable.readonly": "#000000",
    "variable.readonly.defaultLibrary": "#000000",
    "parameter": "#000000",
    "property": "#000000",
    "property.readonly": "#000000",
    "enumMember": {
      "fontStyle": "bold",
      "foreground": "#000000"
    },
    "event": "#000000"
  }
}
